<style>
  #subscribe img {
    display: inline;
    height: 2.8rem;
  }
</style>
<div id="subscribe">
{% for service, url in site.podcast.subscribe %}
{% if url %}
  <a href="{{ url }}"><img src="{{ asset('badge/'~ service ~'.svg')|url }}" alt="Listen on {{ service|capitalize }}"/></a>
{% endif %}
{% endfor %}
</div>
